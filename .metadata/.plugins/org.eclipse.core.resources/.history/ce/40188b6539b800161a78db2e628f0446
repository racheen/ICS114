package model;

import javax.persistence.Entity;
import javax.persistence.Id;

@Entity (name="MeralcoPaymentBill")
public class Electricity {
	
	@Id
	private int id;
	private int killowattHours;
	private double initialBill;
	private double finalBill;
	public int getKillowattHours() {
		return killowattHours;
	}
	public void setKillowattHours(int killowattHours) {
		this.killowattHours = killowattHours;
	}
	public double getInitialBill() {
		return initialBill;
	}
	public void setInitialBill(double initialBill) {
		this.initialBill = initialBill;
	}
	
	public double getFinalBill() {
		return finalBill;
	}
	public void setFinalBill(double finalBill) {
		this.finalBill = finalBill;
	}
	
	
	
}
